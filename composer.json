{
    "name": "jshxl/tree-select",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0",
        "laravel/nova": "~4.0",
        "jshxl/primevue": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Jshxl\\TreeSelect\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jshxl\\TreeSelect\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/chriswest101/nova-primevue"
        }
    ]
}
